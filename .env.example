# Azure IoT Hub Configuration
# Copy this file to .env and fill in your actual values

# Device Connection String from Azure IoT Hub
AZURE_IOT_CONNECTION_STRING="HostName=YOUR_IOTHUB_NAME.azure-devices.net;DeviceId=YOUR_DEVICE_ID;SharedAccessKey=YOUR_SHARED_ACCESS_KEY"

# Azure Function Configuration
IOTHUB_CONNECTION_STRING="HostName=YOUR_IOTHUB_NAME.azure-devices.net;SharedAccessKeyName=service;SharedAccessKey=YOUR_SERVICE_KEY"

# Cosmos DB Configuration
COSMOSDB_CONNECTION_STRING="AccountEndpoint=https://YOUR_COSMOSDB_ACCOUNT.documents.azure.com:443/;AccountKey=YOUR_ACCOUNT_KEY;"

# Azure Functions Event Hub Connection
AzureWebJobsEventHubConnection="HostName=YOUR_IOTHUB_NAME.azure-devices.net;SharedAccessKeyName=service;SharedAccessKey=YOUR_SERVICE_KEY"